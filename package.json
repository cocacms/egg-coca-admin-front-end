{
  "name": "coca-admin-front-end",
  "version": "2.0.1",
  "description": "Coca rapid development framework Admin frontend For Amoy Freesailing Company Base On EggJs",
  "private": true,
  "scripts": {
    "start": "umi dev",
    "start-dev": "cross-env UMI_ENV=dev umi dev",
    "start:ssl": "cross-env UMI_ENV=ssl umi dev",
    "build": "cross-env UMI_ENV=prod umi build",
    "build-dev": "cross-env UMI_ENV=dev umi build",
    "test": "umi test",
    "lint": "eslint --ext .js src",
    "precommit": "lint-staged",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s -r 0"
  },
  "dependencies": {
    "@umijs/hooks": "^1.7.1",
    "axios": "^0.19.0",
    "braft-editor": "^2.3.7",
    "file-saver": "^2.0.2",
    "for-editor": "^0.3.5",
    "loadsh": "^0.0.4",
    "mobx": "^5.10.1",
    "mobx-react": "^6.1.1",
    "mobx-utils": "^5.5.4",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "styled-components": "^4.3.2",
    "xlsx": "^0.15.1"
  },
  "devDependencies": {
    "@commitlint/cli": "^8.3.5",
    "@commitlint/config-conventional": "^8.3.4",
    "@types/file-saver": "^2.0.1",
    "@types/jest": "^25.1.3",
    "@types/lodash": "^4.14.149",
    "@types/node": "^13.7.4",
    "@types/react": "^16.9.22",
    "@types/react-dom": "^16.9.5",
    "@types/react-test-renderer": "^16.9.2",
    "@types/styled-components": "^5.0.0",
    "@umijs/preset-react": "^1.2.2",
    "babel-eslint": "^9.0.0",
    "commitizen": "^4.0.4",
    "conventional-changelog-cli": "^2.0.31",
    "cz-conventional-changelog": "^3.1.0",
    "eslint": "^5.4.0",
    "eslint-config-umi": "^1.4.0",
    "eslint-plugin-flowtype": "^2.50.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^5.1.1",
    "eslint-plugin-react": "^7.11.1",
    "eslint-plugin-react-hooks": "^2.3.0",
    "husky": "^0.14.3",
    "lint-staged": "^7.2.2",
    "react-test-renderer": "^16.7.0",
    "umi": "^3.0.0"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "lint-staged": {
    "*.{js,jsx}": [
      "eslint --fix",
      "git add"
    ]
  },
  "engines": {
    "node": ">=8.0.0"
  }
}
